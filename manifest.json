{
  "name": "Universal Cleaner",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "cookies",
    "scripting",
    "tabs",
    "browsingData"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "devtools_page": "devtools.html",
  "icons": {
    "16":  "icons/icon-16.png",
    "48":  "icons/icon-48.png",
    "128": "icons/icon-128.png",
    "512": "icons/icon-512.png"
  },
  "action": {
    "default_icon": {
        "16":  "icons/icon-16.png",
        "48":  "icons/icon-48.png",
        "128": "icons/icon-128.png"
    }
  }
}